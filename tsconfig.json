{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": ["src/*"]
    },
    "types": [
      "node",
      "@cloudflare/workers-types/2023-07-01",
      "ewelink-api-next"
    ],
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"]
}
